<ul>
  <% @search_from_db.each_with_index do |search, i| %>
    <li>
      <h6><%= search.name %></h6>
      <!-- Modal Trigger -->
      <a class="modal-trigger" href="#modal<%= i %>"><%= search.name %></a>
      <img src='<%= search.image_url == "#" ? "http://loremflickr.com/320/240/music?random=4" : search.image_url %>' >
      <% if !@is_artist %>
        <h6><%= search.artist %></h6>
      <% end %>
      <!-- Modal Structure -->
      <div id="modal<%= i %>" class="modal">
        <div class="modal-content">
          <h4>Modal Header</h4>
          <p>A bunch of text</p>
        </div>
        <div class="modal-footer">
          <a href="#!" class=" modal-action modal-close waves-effect waves-green btn-flat">Agree</a>
        </div>
      </div>

      <!-- <img src='<%#= result[:artworkUrl100] %>' > --> <!-- Grab generic image for artist? -->
    </li>

  <% end %>
</ul>
